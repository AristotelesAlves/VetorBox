generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Vetores {
  ID_Vetor String @id @default(uuid())
  ID_Usuario Int
  Nome String
  URL_IMG String
  URL_SVG String
  URL_EPS String
  URL_PNG String
  Downloads Int
  create_at DateTime
  Update_at DateTime

  @@map("vetor")
  user Users @relation(fields: [ID_Usuario], references: [ID]) // one to many
  categorias Categorias[] // many to many
}

model Categorias {
  ID_Categoria Int @id @default(autoincrement())
  Nome String
  vetores Vetores[]
}

model Users {
  ID Int @id @default(autoincrement())
  Name String
  Email String
  Passworld String
  Vetores Vetores[]
}
